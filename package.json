{
  "name": "@mudas/weui.js",
  "version": "0.0.1",
  "author": "mudas",
  "license": "MIT",
  "description": "A lot of modifications have been made by Weui. If you don't like it, please use the original version.",
  "main": "./dist/weui.min.js",
  "keywords": [
    "weui",
    "template",
    "javascript",
    "zepto"
  ],
  "homepage": "https://github.com/aosnow/weui.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aosnow/weui.js.git"
  },
  "bugs": {
    "url": "https://github.com/aosnow/weui.js/issues"
  },
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --colors --config build/webpack.example.config.js",
    "build": "npm run clean && npm run lint && cross-env NODE_ENV=production  webpack --config ./build/webpack.example.config.js && cross-env REPORT=false node build/build.js",
    "report": "npm run clean && npm run lint && cross-env NODE_ENV=production  webpack --config ./build/webpack.example.config.js && cross-env REPORT=true node build/build.js",
    "docs": "node ./build/docs.js",
    "deploy": "npm run docs && gh-pages -d docs",
    "lint": "eslint ./src/**/*.js",
    "clean": "rimraf ./dist",
    "start": "npm run serve",
    "server": "node uploader-server/index.js"
  },
  "dependencies": {
    "template_js": "^0.8.0",
    "zepto": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime-corejs2": "^7.6.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "exports-loader": "^0.7.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gh-pages": "^0.12.0",
    "gitbook-cli": "^2.3.2",
    "glob": "^7.1.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc-to-markdown": "^2.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mocha": "^6.2.0",
    "portfinder": "^1.0.24",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.5.4",
    "script-loader": "^0.7.2",
    "style-loader": "^1.0.0",
    "thread-loader": "^2.1.3",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  }
}
